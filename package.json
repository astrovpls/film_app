{
  "name": "react-mobx-ssr",
  "version": "1.0.0",
  "description": "React+Mobx+SSR boilerplate",
  "main": "src/index.js",
  "scripts": {
    "start": "npm-run-all --parallel start-proxy start-server",
    "start-proxy": "webpack serve --mode=development",
    "start-server": "cross-env PORT=3011 node --inspect=9229 src/index.js",
    "start-prod": "npm run build-prod && node src/index.js",
    "update": "npx npm-check-updates -u && npm install",
    "flow": "flow check",
    "build": "webpack",
    "build-prod": "webpack --node-env production",
    "test": "jest --watch"
  },
  "author": "Anton Bystrov",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "mobx": "^6.1.7",
    "mobx-react": "^7.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.13.8",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^5.1.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "react-hot-loader": "^4.13.0",
    "react-test-renderer": "^17.0.1",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
